<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--? add by dingwei.chen start ?-->
    <declare-styleable name="VoiceView">
        <attr format="color" name="background_color" />
        <attr format="color" name="alert_line_color" />
        <attr format="color" name="line_color" />
        <attr format="string|reference" name="cancel_send" localization="suggested"/>
        <attr format="string|reference" name="record_less" localization="suggested"/>
        <attr format="string|reference" name="record_over" localization="suggested"/>
        <attr format="string|reference" name="move_to_cancel" localization="suggested"/>
    </declare-styleable>
    
    <declare-styleable name="AttachLayout">
        <attr format="reference" name="attach_loading"/>
        <attr format="reference" name="attach_error" />
        <attr format="reference" name="attach_unlisten"/>
        <attr format="reference" name="attach_domain" />
        <attr format="reference" name="attach_unlisten_src"/>
        <attr format="reference" name="attach_domain_src" />
        <attr format="reference" name="attach_error_src" />
        <attr format="dimension" name="left_padding" />
        <attr format="dimension" name="right_padding" />
        <attr format="dimension" name="loading_width" />
        <attr format="dimension" name="loading_height" />
        <attr format="boolean" name="attach_right"/>
    </declare-styleable>
    
    <declare-styleable name="RecordButton">
        <attr format="string|reference" name="text_up" localization="suggested"/>
        <attr format="string|reference" name="text_down" localization="suggested"/>
    </declare-styleable>
     <!--? add by dingwei.chen end ?-->
	
    <!-- add by xiangchao.fan for SettingLayout -->
    <declare-styleable name="SettingLayoutBackgroundType">
        <attr name="backgroundType" format="integer"/>
    </declare-styleable>
    
    
        
    <!-- 以下为老代码使用的declare-styleable，发版前将会删除，如果新版本中要使用，请移至此注释之前 -->
    <declare-styleable name="MyEditText">
        <attr format="integer" name="img_background_default" />
        <attr format="integer" name="img_background_hover" />
    </declare-styleable>
    <declare-styleable name="TextImageView">
        <attr format="string" name="rawText" />
        <attr format="dimension" name="textSize" />
        <attr format="color" name="textColor" />
        <attr format="string" name="startDelimiter" />
        <attr format="string" name="endDelimiter" />
    </declare-styleable>
</resources>