package com.renren.mobile.chat.view;

public class BaseTitleState {
	private int mImgId;//图片id
	private String mText;//状态文案
	private int mType;

	public int getmType() {
		return mType;
	}

	public void setmType(int mType) {
		this.mType = mType;
	}

	BaseTitleState(int type,int imgId,String text){
		this.mType = type;
		this.mImgId = imgId;
		this.mText = text;
	}
	
	public int getmImgId() {
		return mImgId;
	}
	public void setmImgId(int mImgId) {
		this.mImgId = mImgId;
	}
	public String getmText() {
		return mText;
	}
	public void setmText(String mText) {
		this.mText = mText;
	}
}
