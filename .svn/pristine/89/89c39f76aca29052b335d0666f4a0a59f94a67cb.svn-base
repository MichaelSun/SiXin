<resources>
    <declare-styleable name="Flipper">
        <attr name="content"
              format="integer"/>
        <attr name="head"
              format="integer"/>
        <attr name="handle"
              format="integer"/>
    </declare-styleable>

    <declare-styleable name="Awesome">
        <attr name="left_offset"
              format="dimension"/>
        <attr name="right_offset"
              format="dimension"/>
        <attr name="top_offset"
              format="dimension"/>
        <attr name="bottom_offset"
              format="dimension"/>
        <attr name="track"
              format="string"/>
        <attr name="tap_back_area"
              format="string"/>
    </declare-styleable>

    <declare-styleable name="ImageTextView">
        <attr name="position"
              format="string"/>
        <attr name="spacing"
              format="dimension"/>
        <attr name="text_gravity"
              format="string"/>
        <attr name="image_gravity"
              format="string"/>
        <attr name="image_width"
              format="dimension"/>
        <attr name="image_height"
              format="dimension"/>
    </declare-styleable>

    <declare-styleable name="StatusImageView">
        <attr name="attached_corner"
              format="string"/>
        <attr name="main_src"
              format="reference"/>
        <attr name="status_src"
              format="reference"/>
        <attr name="main_image_width"
              format="dimension"/>
        <attr name="main_image_height"
              format="dimension"/>
        <attr name="status_image_width"
              format="dimension"/>
        <attr name="status_image_height"
              format="dimension"/>
        <attr name="extend_offset_X"
              format="dimension"/>
        <attr name="extend_offset_Y"
              format="dimension"/>
        <attr name="enable_animation"
              format="boolean"/>
    </declare-styleable>

    <declare-styleable name="Flip3DLayout">
        <attr name="from"
              format="integer"/>
        <attr name="to"
              format="integer"/>
    </declare-styleable>

    <declare-styleable name="FakeChromeIndicator">
        <attr name="space"
              format="dimension"/>
        <attr name="drawable"
              format="reference"/>
        <attr name="selector"
              format="reference"/>
    </declare-styleable>
</resources>