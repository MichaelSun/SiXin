package com.renren.mobile.web.branches.api_11;

import android.webkit.WebView;

/**
 * at 5:44 PM, 4/11/12
 *
 * @author afpro
 */
public class WebViewFragmentOperation {
    public static void onPause(WebView webView) {
        if (webView != null) {
            webView.onPause();
        }
    }

    public static void onResume(WebView webView) {
        if (webView != null) {
            webView.onResume();
        }
    }
}
